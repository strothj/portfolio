    "@babel/plugin-transform-object-assign": "^7.0.0-beta.42",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@types/aws-lambda": "^0.0.34",
    "@types/graphql": "^0.12.7",
    "aws-lambda": "^0.1.2",
    "netlify-lambda": "^1.0.0-babel-7-beta",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
